    "babel": "^5.8.3",
    "hapi": "^8.8.0",
    "mongodb": "^2.0.39"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "karma": "^0.13.2",
    "karma-babel-preprocessor": "^5.2.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^0.2.0",
