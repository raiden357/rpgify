{
  "name": "rpgify",
  "version": "1.0.0",
  "description": "Rpgify allows a user to turn their life into a customized role playing game. Users create skills which hold a level value. For each level the user specifies the achievement required to complete that level. Once that achievement is accomplished they advance a level.",
  "main": "server.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "start": "nodemon",
    "test": "mocha spec/int/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/rquez/rpgify.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^0.8.5",
    "boom": "^3.1.1",
    "hapi": "^12.1.0",
    "hapi-auth-google": "^3.1.0",
    "hapi-auth-jwt": "^4.0.0",
    "hapi-router": "^3.0.1",
    "jsonwebtoken": "^5.5.4",
    "mongodb": "^2.1.4",
    "mongoose": "^4.3.5",
    "nodemon": "^1.8.1",
    "shortid": "^2.2.4"
  },
  "devDependencies": {
    "blipp": "^2.3.0",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
